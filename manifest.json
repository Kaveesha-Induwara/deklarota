{
  "name": "Seesu App",
  "version": "2.7",
  "description": "Seesu is for searching and listening music. Free last.fm player.",
  "app": {
    "launch": {
      "local_path":"index.html"
    }
  },
  "icons": {
	"16": "icons/icon16.png",
	"32": "icons/icon32.png",
	"48": "icons/icon48.png",
	"64": "icons/icon64.png",
	"128": "icons/icon128.png"
  },
  "permissions": [
	"https://*.vk.com/*",
	"http://*.last.fm/*",
	"http://*.audioscrobbler.com/*",
	"http://api.soundcloud.com/*",
	"http://*.seesu.me/*",
	"http://*.vk.com/*"
  ]
}